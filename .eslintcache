roject/src/components/header/Header.tsx",["177","178"],"import React from \"react\";\nimport styles from \"./Header.module.css\";\nimport logo from \"../../assets/logo.svg\";\nimport { Dropdown, Layout, Menu, Button, Typography, Input } from \"antd\";\nimport { GlobalOutlined } from \"@ant-design/icons\";\nimport { useHistory } from 'react-router-dom'\nimport { useTranslation } from 'react-i18next'\nimport { useDispatch } from 'react-redux'\nimport { Dispatch } from 'react'\nimport { LanguageActionTypes, changeLanguageActionCreator, addNewLanguageActionCreator } from '../../redux/language/languageActions'\nimport { useSelector } from '../../redux/hooks'\n\n\nexport const Header: React.FC = () => {\n  const history = useHistory()\n  const language = useSelector((state) => state.language.language)\n  const languageList = useSelector((state) => state.language.languageList)\n  const dispatch = useDispatch()\n  const { t } = useTranslation()\n\n  const menuItemClickHandle = (e) => {\n    if(e.key ===\"new\"){\n      dispatch(addNewLanguageActionCreator(\"新语言\",\"new_language\"))\n    }else{\n      dispatch(changeLanguageActionCreator(e.key))\n    }\n\n  }\n  return (\n    <div className={styles[\"app-header\"]}>\n      <div className={styles[\"top-header\"]}>\n        <div className={styles.inner}>\n          <Typography.Text>让旅游更幸福</Typography.Text>\n          <Dropdown.Button\n            style={{ marginLeft: 15 }}\n            overlay={\n\n              <Menu onClick={menuItemClickHandle}>\n                {languageList.map((mm) => {\n                  return <Menu.Item key={mm.code}>{mm.name}</Menu.Item>\n                })}\n                <Menu.Item key={\"new\"}>\n                  {t(\"header.add_new_language\")}\n                </Menu.Item>\n              </Menu>\n\n            }\n            icon={<GlobalOutlined />}\n          >\n            {language === \"zh\" ? \"中文\" : \"English\"}\n          </Dropdown.Button>\n          <Button.Group className={styles[\"button-group\"]}>\n            <Button onClick={() => { history.push('/register') }}> {t(\"header.register\")}</Button>\n            <Button onClick={() => { history.push('/signIn') }}> {t(\"header.signin\")}</Button>\n          </Button.Group>\n        </div>\n      </div>\n      <Layout.Header className={styles[\"main-header\"]}>\n        <span onClick={() => history.push('/')}>\n          <img src={logo} alt=\"\" className={styles[\"App-logo\"]} />\n          <Typography.Title level={3} className={styles.title}>\n          {t(\"header.title\")}\n        </Typography.Title>\n        </span>\n        <Input.Search\n          placeholder={\"请输入旅游目的地,主题,或关键字\"}\n          className={styles[\"search-input\"]}\n        ></Input.Search>\n      </Layout.Header>\n      <Menu mode={\"horizontal\"} className={styles[\"main-menu\"]}>\n      <Menu.Item key=\"1\"> {t(\"header.home_page\")} </Menu.Item>\n        <Menu.Item key=\"2\"> {t(\"header.weekend\")} </Menu.Item>\n        <Menu.Item key=\"3\"> {t(\"header.group\")} </Menu.Item>\n        <Menu.Item key=\"4\"> {t(\"header.backpack\")} </Menu.Item>\n        <Menu.Item key=\"5\"> {t(\"header.private\")} </Menu.Item>\n        <Menu.Item key=\"6\"> {t(\"header.cruise\")} </Menu.Item>\n        <Menu.Item key=\"7\"> {t(\"header.hotel\")} </Menu.Item>\n        <Menu.Item key=\"8\"> {t(\"header.local\")} </Menu.Item>\n        <Menu.Item key=\"9\"> {t(\"header.theme\")} </Menu.Item>\n        <Menu.Item key=\"10\"> {t(\"header.custom\")} </Menu.Item>\n        <Menu.Item key=\"11\"> {t(\"header.study\")} </Menu.Item>\n        <Menu.Item key=\"12\"> {t(\"header.visa\")} </Menu.Item>\n        <Menu.Item key=\"13\"> {t(\"header.enterprise\")} </Menu.Item>\n        <Menu.Item key=\"14\"> {t(\"header.high_end\")} </Menu.Item>\n        <Menu.Item key=\"15\"> {t(\"header.outdoor\")} </Menu.Item>\n      </Menu>\n    </div>\n  );\n};\n","/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/components/banner/Banner.tsx",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/components/footer/Footer.tsx",["179"],"import React from \"react\";\nimport {  Layout,Typography } from \"antd\";\nimport {useTranslation,WithTranslation} from 'react-i18next'\n\nexport const Footer: React.FC = () => {\n  const {t} = useTranslation()\n  return (\n    <div>\n      <Layout.Footer>\n        <Typography.Title\n          level={3}\n          style={{ textAlign: \"center\" }}\n          type={\"success\"}\n        >\n          {t('footer.detail')}\n        </Typography.Title>\n      </Layout.Footer>\n    </div>\n  );\n};\n","/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/components/hotTourist/ProductImageInfo.tsx",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/components/hotTourist/HotTourist.tsx",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/components/company/Company.tsx",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/components/slideItem/SlideItem.tsx",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/mock/slideItem.ts",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/components/header/Header.class.tsx",["180"],"\nimport React from \"react\";\nimport styles from \"./Header.module.css\";\nimport logo from \"../../assets/logo.svg\";\nimport { Dropdown, Layout, Menu, Button, Typography, Input } from \"antd\";\nimport { GlobalOutlined } from \"@ant-design/icons\";\nimport { withRouter, RouteComponentProps } from \"react-router-dom\"\nimport store, { RootState } from '../../redux/store'\nimport { withTranslation, WithTranslation } from 'react-i18next'\nimport { addNewLanguageActionCreator, changeLanguageActionCreator } from '../../redux/language/languageActions'\nimport { Dispatch } from 'redux'\n\nimport { connect } from \"react-redux\"\n/**\n * \n * 使用redux处理数据\n *\n * 类组件 使用 i18n  \n */\n\n\n\nconst mapStateToProps = (state: RootState) => {\n  return {\n    language: state.language,\n    languageList: state.languageList\n  }\n}\n\nconst mapDispatchToProps = (dispatch: Dispatch) => {\n  return {\n    changeLanguage: (code:\"en\" | \"zh\") => {\n      const action = changeLanguageActionCreator(code)\n      dispatch(action)\n    },\n    addLanguage: (name:string, code:string) => {\n      const action = addNewLanguageActionCreator(name,code);\n      dispatch(action)\n    }\n  }\n}\n\ntype PropsType = RouteComponentProps & WithTranslation & ReturnType<typeof mapStateToProps> & ReturnType<typeof mapDispatchToProps>\nclass HeaderComponents extends React.Component<PropsType>{\n\n  changeMenuItemHandle=(e) =>{\n    if (e.key === \"new\") {\n      // const action = addNewLanguageActionCreator(\"newLanguage\", \"新语言\")\n      // store.dispatch(action)\n      this.props.addLanguage(\"newLanguage\", \"新语言\")\n    } else {\n      // const action = changeLanguageActionCreator(e.key)\n      // store.dispatch(action)\n      this.props.changeLanguage(e.key)\n    }\n\n  }\n  render() {\n    const { history, t } = this.props;   // 获取路由history 对象\n    return (\n      <div className={styles[\"app-header\"]}>\n        <div className={styles[\"top-header\"]}>\n          <div className={styles.inner}>\n            <Typography.Text>{t(\"header.slogan\")}</Typography.Text>\n            <Dropdown.Button\n              style={{ marginLeft: 15 }}\n              overlay={\n                <Menu onClick={this.changeMenuItemHandle}>\n                  {this.props.languageList.map(mm => {\n                    return <Menu.Item key={mm.code}>{mm.name}</Menu.Item>\n                  })}\n                  <Menu.Item key=\"new\">{t(\"header.add_new_language\")}</Menu.Item>\n                </Menu>\n              }\n              icon={<GlobalOutlined />}\n            >\n              {this.props.language === \"zh\" ? \"中文\" : \"English\"}\n            </Dropdown.Button>\n            <Button.Group className={styles[\"button-group\"]}>\n              <Button onClick={() => { history.push('register') }}>{t(\"header.register\")}</Button>\n              <Button onClick={() => { history.push('signIn') }}>{t(\"header.signin\")}</Button>\n            </Button.Group>\n          </div>\n        </div>\n        <Layout.Header className={styles[\"main-header\"]}>\n          <span onClick={() => { history.push('/') }} >\n            <img src={logo} alt=\"\" className={styles[\"App-logo\"]} />\n            <Typography.Title level={3} className={styles.title}>\n              {t(\"header.title\")}\n            </Typography.Title>\n          </span>\n          <Input.Search\n            placeholder={\"请输入旅游目的地,主题,或关键字\"}\n            className={styles[\"search-input\"]}\n          ></Input.Search>\n        </Layout.Header>\n        <Menu mode={\"horizontal\"} className={styles[\"main-menu\"]}>\n          <Menu.Item key=\"1\"> {t(\"header.home_page\")} </Menu.Item>\n          <Menu.Item key=\"2\"> {t(\"header.weekend\")} </Menu.Item>\n          <Menu.Item key=\"3\"> {t(\"header.group\")} </Menu.Item>\n          <Menu.Item key=\"4\"> {t(\"header.backpack\")} </Menu.Item>\n          <Menu.Item key=\"5\"> {t(\"header.private\")} </Menu.Item>\n          <Menu.Item key=\"6\"> {t(\"header.cruise\")} </Menu.Item>\n          <Menu.Item key=\"7\"> {t(\"header.hotel\")} </Menu.Item>\n          <Menu.Item key=\"8\"> {t(\"header.local\")} </Menu.Item>\n          <Menu.Item key=\"9\"> {t(\"header.theme\")} </Menu.Item>\n          <Menu.Item key=\"10\"> {t(\"header.custom\")} </Menu.Item>\n          <Menu.Item key=\"11\"> {t(\"header.study\")} </Menu.Item>\n          <Menu.Item key=\"12\"> {t(\"header.visa\")} </Menu.Item>\n          <Menu.Item key=\"13\"> {t(\"header.enterprise\")} </Menu.Item>\n          <Menu.Item key=\"14\"> {t(\"header.high_end\")} </Menu.Item>\n          <Menu.Item key=\"15\"> {t(\"header.outdoor\")} </Menu.Item>\n\n        </Menu>\n      </div>\n    )\n  }\n}\n\nexport const Header = connect(mapStateToProps, mapDispatchToProps)(\n  withTranslation()(withRouter(HeaderComponents))\n);\n",["181","182"],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/redux/store.ts",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/utils/constant.ts",[],["183","184"],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/i18n/configs.ts",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/redux/language/languageReducer.ts",["185"],"/**\n * 創建reducer步驟\n */\nimport {CHANGE_LANGUAGE,ADD_NEW_LANGUAGE,LanguageActionTypes} from './languageActions'\nimport i18n from 'i18next'\nexport interface languageState {\n  language: 'en' | 'zh',\n  languageList: { \"name\": string; \"code\": string }[]\n}\n\nconst languageDefault: languageState = {\n  language: 'zh',\n  languageList: [{\n    name: \"中文\", code: 'zh'\n  }, { name: \"English\", code: 'en' }]\n}\n\nexport default (state = languageDefault, action:LanguageActionTypes)=>{\n  switch(action.type){\n    case CHANGE_LANGUAGE:\n      i18n.changeLanguage(action.payload)\n      return {...state, language:action.payload }\n    case ADD_NEW_LANGUAGE:\n      return {...state,languageList:[...state.languageList,action.payload] }\n      default:\n        return state\n  }\n}","/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/redux/language/languageActions.ts",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/redux/hooks.ts",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/redux/recommendProducts/recommendProductsReducers.ts",["186"],"import { FETCH_RECOMMEND_PRODUCTS_START, FETCH_RECOMMEND_PRODUCTS_SUCCESS, FETCH_RECOMMEND_PRODUCTS_FAIL, RecommendProductAction } from './recommendProductsActions'\n\ninterface RecommendProductsState {\n  productList: any[],\n  loading: boolean,\n  error: string | null\n}\n\nconst defaultState: RecommendProductsState = {\n  productList: [],\n  loading: true,\n  error: null\n}\n\nexport default (state = defaultState, action: RecommendProductAction) => {\n  switch (action.type) {\n    case FETCH_RECOMMEND_PRODUCTS_START:\n      return {\n        ...state, loading: true\n      }\n    case FETCH_RECOMMEND_PRODUCTS_SUCCESS:\n      return { ...state, loading: false, productList: action.payload }\n    case FETCH_RECOMMEND_PRODUCTS_FAIL:\n      return { ...state, loading: false, error: action.payload }\n    default:\n      return state\n  }\n}","/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/redux/recommendProducts/recommendProductsActions.ts",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/redux/middleWares/actionLog.ts",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/components/productIntro/index.ts",[],"/Users/zanzanya/Documents/03_Project/webtest/1-web/6_react/react-trave-project/src/components/productIntro/ProductIntro.tsx",["187","188"],"import React from 'react'\nimport styles from './ProductIntro.module.css'\nimport { Typography, Image, Rate, Carousel, Table } from 'antd'\nimport { ColumnsType } from 'antd/es/table'\nimport { JsxAttribute } from 'typescript'\n\ninterface PropsType {\n  title: string;\n  shortDescription: string;\n  price: string | number;\n  coupons: string;\n  points: string;\n  discount: string;\n  rating: string | number;\n  pictures: string[];\n}\n\n const columns: ColumnsType<RowType> = [{\n  title: \"title\",\n  dataIndex: \"title\",\n  key: \"title\",\n  align: \"left\",\n  width: 120\n}, {\n  title: \"description\",\n  dataIndex: \"description\",\n  key: \"description\",\n  align: \"center\"\n}]\n\ninterface RowType {\n  title: string,\n  description: string | number | JSX.Element,\n  key: number\n}\